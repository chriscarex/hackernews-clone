{
  "name": "HackernewsNews",
  "version": "0.0.1",
  "description": "Server for Hackernews News",
  "main": "serverless.yml",
  "scripts": {
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint --fix ./src",
    "pretty": "prettier --write \"src/**/*.js\"",
    "server": "babel-node ./src/index.js",
    "dev": "npm-run-all server"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/data-lens/dl-web-site-lambdas"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/data-lens/dl-web-site-lambdas"
  },
  "homepage": "https://bitbucket.org/data-lens/dl-web-site-lambdas",
  "dependencies": {
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-server-utils": "^1.1.0",
    "fast.js": "^0.1.1",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "object-hash": "^1.3.1",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-rewire": "^1.2.0",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "link-module-alias": "^1.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "shx": "^0.3.2",
    "sinon": "^7.5.0"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFiles": [],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/__test__/*.test.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run test",
      "post-commit": "git update-index -g"
    }
  }
}
